    "cuid": "^1.3.8",
    "express": "^4.17.1",
    "google-map-react": "^1.1.4",
    "gotham-fonts": "^1.0.3",
    "http": "0.0.0",
    "https": "^1.0.0",
    "intl": "^1.2.4",
    "intl-locales-supported": "^1.4.5",
    "isomorphic-fetch": "^2.2.1",
    "limax": "^1.7.0",
    "mandrill-api": "^1.0.45",
    "mongoose": "^4.13.19",
    "nodemailer": "^6.3.0",
    "prop-types": "^15.7.2",
    "querystring": "^0.2.0",
    "react": "^16.9.0",
    "react-credit-cards": "^0.7.0",
    "react-dom": "^16.9.0",
    "react-google-maps": "^9.4.5",
    "react-helmet": "^5.2.1",
    "react-intl": "^2.9.0",
    "react-payment-icons": "^1.2.6",
    "react-phone-input-2": "^2.9.5",
    "react-places-autocomplete": "^7.2.1",
    "react-redux": "^4.4.10",
    "react-router": "^3.2.3",
    "react-spinners": "^0.5.13",
    "react-stripe-elements": "^4.0.0",
    "react-switch": "^5.0.1",
    "reactstrap": "^8.0.1",
