    "ava": "^0.15.2",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^7.1.5",
    "babel-plugin-webpack-loaders": "^0.9.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-native-modules": "^6.6.0",
